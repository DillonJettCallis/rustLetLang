
public fun main(): Float = {
    let outside = 2;

    let x = higher({a, b -> Float => a + b * outside});
    x
}

fun higher(op: {Float, Float -> Float}): Float = {
  let first = op(2, 4)
  let second = op(first, 3)
  let third = 4 + 3 * 1 - second
  let fourth = first + second * third
  op(fourth, 2)
}